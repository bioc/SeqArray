	Keywords = {Principal Component Analysis},
	Language = {eng},
	Month = {May},
	Number = {4},
	Pages = {276--293},
	Title = {Robust inference of population structure for ancestry prediction and correction of stratification in the presence of relatedness.},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/25810074},
	Volume = {39},
	Year = {2015},
	Bdsk-Url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/25810074},
