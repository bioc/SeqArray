	Keywords = {Software},
	Language = {eng},
	Month = {Jul},
	Number = {1},
	Pages = {82--93},
	Title = {Rare-variant association testing for sequencing data with the sequence kernel association test.},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/21737059},
