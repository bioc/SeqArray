	Keywords = {United States},
	Language = {eng},
	Month = {Nov},
	Number = {11},
	Pages = {1468--1476},
	Title = {Measures of human population structure show heterogeneity among genomic regions.},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/16251456},
	Volume = {15},
	Year = {2005},
	Bdsk-Url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/16251456},
